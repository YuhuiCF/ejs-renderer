{
  "name": "ejs-renderer",
  "version": "0.1.4",
  "description": "Use EJS to render HTML files by reading individual configuration files",
  "main": "app/index.js",
  "scripts": {
    "jasmine:unit": "jasmine JASMINE_CONFIG_PATH=./test/unit/jasmine.json",
    "nyc:integration": "nyc --report-dir ./coverage/integration bash integration-test.sh || true",
    "nyc:unit": "nyc npm run jasmine:unit",
    "pretest": "rm -rf ./coverage",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "npm run nyc:integration",
    "test:unit": "npm run nyc:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuhuiCF/ejs-renderer.git"
  },
  "author": "Yuhui.Xiong@mcon-group.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YuhuiCF/ejs-renderer/issues"
  },
  "homepage": "https://github.com/YuhuiCF/ejs-renderer#readme",
  "nyc": {
    "all": true,
    "cache": true,
    "exclude": [
      "**/node_modules/**"
    ],
    "include": [
      "**/app/**"
    ],
    "report-dir": "./coverage/unit",
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "ejs": "2.5.6",
    "fs-extra": "4.0.0",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "eslint": "4.2.0",
    "eslint-config-google": "0.9.1",
    "jasmine": "2.6.0",
    "nyc": "11.0.3",
    "proxyquire": "1.8.0"
  },
  "bin": {
    "ejs-renderer": "./bin/ejs-renderer"
  }
}
